@base <https://w3id.org/fair-ease/ns/endpoint-types> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Metadata
<> a owl:Ontology ;
    rdfs:label "FAIR-EASE Endpoint Types Ontology" ;
    rdfs:comment "FAIR-EASE endpoint types ontology for dcat:conformsTo usage." .

# Endpoint Types Definitions

<#metadata> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "metadata" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#direct-download> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "direct-download" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#landing-page> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "landing-page" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#folder-path> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "folder-path" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#ftp> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "ftp" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#http> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "http" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#https> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "https" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <> .

<#opendap> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "opendap" ;
    rdfs:comment "May be used in both dcat:Service and dcat:Distribution." ;
    rdfs:isDefinedBy <> .

<#stac> a owl:Class, rdfs:Resource, dc:Standard ;
    rdfs:label "stac" ;
    rdfs:comment "May be used in both dcat:Service and dcat:Distribution." ;
    rdfs:isDefinedBy <> .
