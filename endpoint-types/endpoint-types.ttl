@base <https://w3id.org/fair-ease/ns/endpoint-types> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Collection Metadata
<> a skos:Collection ;
    skos:prefLabel "FAIR-EASE Endpoint Types" ;
    rdfs:comment "FAIR-EASE endpoint types for dcat:conformsTo usage." .
    skos:member <#metadata>,
                <#direct-download>,
                <#landing-page>,
                <#folder-path>,
                <#ftp>,
                <#http>,
                <#https>,
                <#opendap>,
                <#stac> .

# Endpoint Types Definitions
<#metadata> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "metadata" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#direct-download> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "direct-download" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#landing-page> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "landing-page" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#folder-path> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "folder-path" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#ftp> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "ftp" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#http> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "http" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#https> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "https" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    skos:inScheme <> .

<#opendap> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "opendap" ;
    rdfs:comment "May be used in both dcat:Service and dcat:Distribution." ;
    skos:inScheme <> .

<#stac> a skos:Concept, rdfs:Resource, dc:Standard ;
    skos:prefLabel "stac" ;
    rdfs:comment "May be used in both dcat:Service and dcat:Distribution." ;
    skos:inScheme <> .
