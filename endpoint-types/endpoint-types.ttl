@base <https://w3id.org/fair-ease/ns/endpoint-types> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Metadata
<Ontology> a owl:Ontology ;
    rdfs:comment "FAIR-EASE endpoint types ontology for dcat:conformsTo usage." .

# Endpoint Types Definitions

<#metadata> a rdfs:Resource, dc:Standard ;
    rdfs:label "metadata" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#direct-download> a rdfs:Resource, dc:Standard ;
    rdfs:label "direct-download" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#landing-page> a rdfs:Resource, dc:Standard ;
    rdfs:label "landing-page" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#folder-path> a rdfs:Resource, dc:Standard ;
    rdfs:label "folder-path" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#ftp> a rdfs:Resource, dc:Standard ;
    rdfs:label "ftp" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#http> a rdfs:Resource, dc:Standard ;
    rdfs:label "http" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#https> a rdfs:Resource, dc:Standard ;
    rdfs:label "https" ;
    rdfs:comment "May be used in dcat:Distribution only." ;
    rdfs:isDefinedBy <Ontology> .

<#opendap> a rdfs:Resource, dc:Standard ;
    rdfs:label "opendap" ;
    rdfs:comment "May be used in both dcat:Service and dcat:Distribution." ;
    rdfs:isDefinedBy <Ontology> .

<#stac> a rdfs:Resource, dc:Standard ;
    rdfs:label "stac" ;
    rdfs:comment "May be used in both dcat:Service and dcat:Distribution." ;
    rdfs:isDefinedBy <Ontology> .
